        "circomlibjs": "0.0.8",
        "poseidon-lite": "0.2.0",
        "rollup-plugin-cleanup": "^3.2.1",
        "rollup-plugin-polyfill-node": "^0.13.0",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-typescript2": "^0.31.2"
